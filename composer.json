{
    "name": "francescomalatesta/wolframalphaphp",
    "description": "A PHP Wrapper for Wolfram|Alpha APIs (v2).",
    "version": "1.0.0",
    "homepage": "https://github.com/andreacivita/wolframalphaphp",
    "license": "MIT",
    "authors": [
	{
        "name": "Andrea Civita",
        "email": "andreacivita994@gmail.com",
        "homepage": "https://andreacivita.it",
        "role": "Developer & Mantainer"
    },
	{
        "name": "Francesco Malatesta",
        "email": "francescomalatesta@live.it",
        "homepage": "http://www.hellofrancesco.com",
        "role": "Original Author"
    }],
    "autoload": {
        "psr-4": {
            "WolframAlpha\\": "src/"
        }
    },
    "require": {
        "php": "^7.3.0",
        "illuminate/collections": "^8.20"
    },
	"require-dev" : {
		"phpunit/phpunit": "^7",
        "friendsofphp/php-cs-fixer": "^2.17",
        "brainmaestro/composer-git-hooks": "^2.8"
	},
    "extra": {
      "hooks": {
        "pre-commit": [
          "echo committing as $(git config user.name)",
          "php-cs-fixer fix ."
        ]
      }
    }
}
